extends ../../layout/index
block cuerpocontenido
    br
    h1(class="text-center")=pagina
    br
    div(class="row justify-content-center")
        each hb in habitacion
            div(class="col")
                div(class="card")
                    img(src="/img/habitacion.png",class="card-img-top")
                    div(class="card-body align-self-center")
                        h6=`Piso:${hb.piso}`
                        h6=`Nombre:${hb.nombre}`
                        h6=`Refrigerador:${hb.refrigerador}`
                        h6=`Sucursal:${hb.hotele.nombre}`
                        